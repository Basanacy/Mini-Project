# imports
#:import NoTransition kivy.uix.screenmanager.NoTransition
#:import SlideTransition kivy.uix.screenmanager.SlideTransition




# SignUp Classes
<GetStarted>:
    name: "GetStarted"
    FloatLayout:
        Image:
            source: 'resources/hms_logo.png'
            size_hint: (0.8,0.525)
            pos_hint: {"x": 0.11, "top": 0.85}
        
        Button:
            text: "Lets Start!"
            font_size: 15
            color: (1, 1, 1,1)
            italic: True
            background_color: (0/255, 153/255, 204/255, 1)
            size_hint: (0.3,0.08)
            pos_hint: {"x":0.36, "top": 0.35}
            on_release:
                root.start_program()
                app.root.transition = SlideTransition(direction = "up")
                app.root.current = "PatientSignUp"

        Button:
            text: "Ahammad Shawki 8"
            font_size: 15
            color: (51/255, 153/255, 255/255,1)
            underline: True
            background_color: (1,1,1,0)
            size_hint: (0.3,0.08)
            pos_hint: {"x":0.36, "top": 0.2}
            on_release:
                root.go_to_website()


            
<AdminSignUp>:
    name: "AdminSignUp"

    username: username
    fullname: fullname
    email: email
    date_of_birth: date_of_birth
    password: password

    FloatLayout:
        Label:
            text: "SignUp Form"
            font_size: 30
            color: (0.4, 0.4, 0.4,1)
            pos_hint: {"top": 1.4}

        Label:
            text: "Choose Your Category"
            font_size: 20
            color: (51/255, 153/255, 255/255,1)
            pos_hint: {"top": 1.34}

        Button:
            id: admin_category
            text: "Admin"
            color: (51/255, 153/255, 255/255,1)
            size_hint: (0.2,0.08)
            pos_hint: {"x":0.1, "top": 0.81}

        Button:
            id: doctor_category
            text: "Doctor"
            size_hint: (0.2,0.08)
            pos_hint: {"x":0.3, "top": 0.81}
            on_release: 
                app.root.transition = NoTransition()
                app.root.current = "DoctorSignUp"

        Button:
            id: patient_category
            text: "Patient"
            size_hint: (0.2,0.08)
            pos_hint: {"x":0.5, "top": 0.81}
            on_release: 
                app.root.transition = NoTransition()
                app.root.current = "PatientSignUp"

        Button:
            id: employee_category
            text: "Employee"
            size_hint: (0.2,0.08)
            pos_hint: {"x":0.7, "top": 0.81}
            on_release: 
                app.root.transition = NoTransition()
                app.root.current = "EmployeeSignUp"

        GridLayout:   
            cols: 2 
            size_hint: (0.6,0.4)   
            pos_hint: {"x":0.2, "top": 0.7}
            Label:
                text: "Username: "
                color: (51/255, 153/255, 255/255,1)
            TextInput:
                id: username
                multiline: False
                background_color: (1,1,1,1)

            Label:
                text: "Fullname: "
                color: (51/255, 153/255, 255/255,1)
            TextInput:
                id: fullname
                multiline: False

            Label:
                text: "Email: "
                color: (51/255, 153/255, 255/255,1)
            TextInput:
                id: email
                multiline: False

            Label:
                text: "Date of Birth: "
                color: (51/255, 153/255, 255/255,1)
            TextInput:
                id: date_of_birth
                text: "YYYY-MM-DD"
                multiline: False

            Label:
                text: "Password: "
                color: (51/255, 153/255, 255/255,1)
            TextInput:
                id: password
                password: True
                multiline: False

        Button: 
            text: "Already have an account? Log in."
            font_size: 15
            color: (51/255, 153/255, 255/255,1)
            background_color: (1,1,1,0)
            underline: True
            size_hint: (0.3,0.08)
            pos_hint: {"x":0.36, "top": 0.25}
            on_release:
                app.root.transition = SlideTransition(direction = "left")
                app.root.current = "AdminLogin"
            
        Button: 
            text: "Submit"
            font_size: 18
            color: (1, 1, 1,1)
            italic: True
            background_color: (0/255, 153/255, 204/255, 1)
            size_hint: (0.3,0.08)
            pos_hint: {"x":0.36, "top": 0.15}
            on_release: 
                app.root.transition = SlideTransition(direction = "left")
                root.signup_to_database()
                



